        <meta name="google-signin-client_id" content="846950957562-b8v9qpu65l75q3htr9d6jstu66j21ucm.apps.googleusercontent.com">

        <link rel="stylesheet" href="{{ asset('bundles/app/css/bootstrap.min.css') }}">
        <link rel="stylesheet" href="{{ asset('bundles/app/css/animate.css') }}">
        <link rel="stylesheet" href="{{ asset('bundles/app/css/font-awesome.min.css') }}">
        <link rel="stylesheet" href="{{ asset('bundles/app/css/owl.carousel.css') }}">
        <link rel="stylesheet" href="{{ asset('bundles/app/css/owl.theme.css') }}">
        <link rel="stylesheet" href="{{ asset('bundles/app/css/styles.css') }}">
        <link rel="stylesheet" href="{{ asset('bundles/app/css/bootstrap-social.css') }}">
        <link rel="stylesheet" href="{{ asset('bundles/app/css/swiper.min.css') }}">
        <!-- Latest compiled and minified CSS -->
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-select/1.12.2/css/bootstrap-select.min.css">
    </head>
    <body>
        {% block body %}
        {% endblock %}
        {% block javascripts %}

            <script src="{{ asset('//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js')}}"></script>
            <script src="{{ asset('bundles/app/js/bootstrap.min.js') }}"></script>
          <!--  <script src="{{ asset('bundles/app/js/owl.carousel.min.js') }}"></script>-->
            <script src="{{ asset('bundles/app/js/waypoints.min.js') }}"></script>
            <script src="{{ asset('bundles/app/js/jquery.counterup.min.js') }}"></script>
            <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
            <script src="https://apis.google.com/js/platform.js" async defer></script>
            <script src="{{ path('fos_js_routing_js', { callback: 'fos.Router.setData' }) }}"></script>
            <script src="{{ asset('bundles/app/js/script.js') }}"></script>
            <!-- Latest compiled and minified JavaScript -->
            <script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-select/1.12.2/js/bootstrap-select.min.js"></script>

            <!-- carousel -->
            <script src="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.0.0-beta.3/owl.carousel.min.js"></script>
            <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.0.0-beta.3/assets/owl.carousel.min.css">
            <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.0.0-beta.3/assets/owl.theme.default.min.css">

            <!-- jQuery REVOLUTION Slider  -->
            <script type="text/javascript" src="{{ asset('bundles/app/rs-plugin/js/jquery.themepunch.plugins.min.js') }}"></script>
            <script type="text/javascript" src="{{ asset('bundles/app/rs-plugin/js/jquery.themepunch.revolution.min.js') }}"></script>
            <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCPjWQfwUf6AEAOuCjMESsg0YAiFTcqBFY&libraries=geometry"></script>

            <!-- swipe -->
            <script type="text/javascript" src="{{ asset('bundles/app/js/jquery.touchSwipe.min.js') }}"></script>
            <script type="text/javascript" src="{{ asset('bundles/app/js/swiper.jquery.min.js') }}"></script>

            <script>
                $(document).ready(function() {
                    appMaster.preLoader();
                });

                var swiper = new Swiper('.swiper-container', {
                    pagination: '.swiper-pagination',
                    nextButton: '.swiper-button-next',
                    prevButton: '.swiper-button-prev',
                    slidesPerView: 1,
                    paginationClickable: true,
                    spaceBetween: 30,
                    loop: true
                });


                $('.btn-signUp').on('click', function () {

                    $('.dropdown').toggleClass('hide');

                })

                $('.fa-chevron-down').on('click', function () {
                    $('.dropdown').removeClass('hidden');
                    $(this).addClass('hidden');
                })
                $('.pubnav').on('click', function () {

                    $('#packages').toggleClass('hidden');

                })

                $('.big-title').on('click', function () {

                    $('.big-title').addClass('hidden-md hidden-lg');
                    $('.big-title1').removeClass('hidden');

                })


                $('.pubs').on('click', function () {

                    if( $('.second').hasClass('visible')){
                        $('.second').toggleClass('hidden');
                        $('.second').toggleClass('visible');
                        $('.about').toggleClass('hidden-xs');
                        $('.about').toggleClass('hidden-sm');
                        $('.about-text').toggleClass('hidden');
                        $('.about-text').toggleClass('visible');

                    }
                    if( $('.third').hasClass('visible')){
                        $('.third').toggleClass('hidden');
                        $('.third').toggleClass('visible');
                        $('.about').toggleClass('hidden-xs');
                        $('.about').toggleClass('hidden-sm');
                        $('.about-text').toggleClass('hidden');
                        $('.about-text').toggleClass('visible');

                    }

                    $('.first').toggleClass('hidden');
                    $('.first').toggleClass('visible');
                    $('.about').toggleClass('hidden-xs');
                    $('.about').toggleClass('hidden-sm');
                    $('.about-text').toggleClass('hidden');
                    $('.about-text').toggleClass('visible');



                })

                $('.beers').on('click', function () {

                    if( $('.first').hasClass('visible')){
                        $('.first').toggleClass('hidden');
                        $('.first').toggleClass('visible');
                        $('.about').toggleClass('hidden-xs');
                        $('.about').toggleClass('hidden-sm');
                        $('.about-text').toggleClass('hidden');
                        $('.about-text').toggleClass('visible');

                    }
                    if( $('.third').hasClass('visible')){
                        $('.third').toggleClass('hidden');
                        $('.third').toggleClass('visible');
                        $('.about').toggleClass('hidden-xs');
                        $('.about').toggleClass('hidden-sm');
                        $('.about-text').toggleClass('hidden');
                        $('.about-text').toggleClass('visible');

                    }
                    $('.second').toggleClass('hidden');
                    $('.second').toggleClass('visible');
                    $('.about').toggleClass('hidden-xs');
                    $('.about').toggleClass('hidden-sm');
                    $('.about-text').toggleClass('hidden');
                    $('.about-text').toggleClass('visible');

                })
                $('.reviews').on('click', function () {

                    if( $('.first').hasClass('visible')){
                        $('.first').toggleClass('hidden');
                        $('.first').toggleClass('visible');
                        $('.about').toggleClass('hidden-xs');
                        $('.about').toggleClass('hidden-sm');
                        $('.about-text').toggleClass('hidden');
                        $('.about-text').toggleClass('visible');

                    }
                    if( $('.second').hasClass('visible')){
                        $('.second').toggleClass('hidden');
                        $('.second').toggleClass('visible');
                        $('.about').toggleClass('hidden-xs');
                        $('.about').toggleClass('hidden-sm');
                        $('.about-text').toggleClass('hidden');
                        $('.about-text').toggleClass('visible');

                    }
                    $('.third').toggleClass('hidden');
                    $('.third').toggleClass('visible');
                    $('.about').toggleClass('hidden-xs');
                    $('.about').toggleClass('hidden-sm');
                    $('.about-text').toggleClass('hidden');
                    $('.about-text').toggleClass('visible');

                })



                $(".beer-item").swipe( {

                    allowPageScroll:"auto",
                    threshold:75,
                    //Generic swipe handler for all directions
                    swipe:function(event, direction, distance, duration, fingerCount, fingerData) {
                        if(direction=="left" && $(this).next().length) {
                            $(this).removeClass('visible');
                            $(this).addClass('hidden');
                            $(this).next().removeClass('hidden');
                           // $(this).next().addClass('visible');
                            $(this).next().toggleClass('animated fadeInRight');
                        }
                        else if(direction=="right" && $(this).prev().length) {
                            $(this).removeClass('visible');
                            $(this).addClass('hidden');
                            $(this).prev().removeClass('hidden');
                            //$(this).prev().addClass('visible');
                            $(this).prev().toggleClass('animated fadeInLeft');
                        }
                    }

                })

                $('.beer-navigation').find('.prev').on('click', function () {
                    var elem =  $(".carousel-inner").find('.visible');
                    if(elem.prev().length && elem.hasClass('visible')){
                        elem.removeClass('visible');
                        elem.addClass('hidden');
                        elem.prev().removeClass('hidden');
                        elem.prev().addClass('visible');
                        elem.prev().toggleClass('animated fadeInRight');
                    }
                })

                $('.beer-navigation').find('.next').on('click', function () {
                    var elem =  $(".carousel-inner").find('.visible');
                    if(elem.next().length){
                        elem.removeClass('visible');
                        elem.addClass('hidden');
                        elem.next().removeClass('hidden');
                        elem.next().addClass('visible');
                        elem.next().toggleClass('animated fadeInLeft');
                    }
                })


                // Counter
                jQuery(document).ready(function($) {
                    $('.statistic-counter').counterUp({
                        delay: 5,
                        time: 500
                    });
                });


                window.fbAsyncInit = function() {
                    FB.init({
                        appId      : '101478166643772',
                        xfbml      : true,
                        version    : 'v2.8'
                    });
                    FB.AppEvents.logPageView();
                };

                (function(d, s, id){
                    var js, fjs = d.getElementsByTagName(s)[0];
                    if (d.getElementById(id)) {return;}
                    js = d.createElement(s); js.id = id;
                    js.src = "//connect.facebook.net/en_US/sdk.js";
                    fjs.parentNode.insertBefore(js, fjs);
                }(document, 'script', 'facebook-jssdk'));
            </script>



        {% endblock %}
    </body>
</html>
